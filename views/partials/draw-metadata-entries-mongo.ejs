
  <script type="text/javascript">

  	function insertText(){

  		var docsObject = <%- docs %>;

      totalhtml = ""
  		i = 0
  		for(i=0; i < docsObject.length; i++){

  			obj = docsObject[i]

        totalhtml += "<div class=\"box\"><div>"

        // get fields from object and append to html object
        for(var key in obj) {

              if(obj.hasOwnProperty(key)){

                var value = obj[key]
                if (key === "folderID") {
                  totalhtml += "<strong><u>" + String(key) + "</strong></u>: <a href='https://<%= hostName %>/minio/data/" + String(value) + "/'>" + String(value) + "</a><br>"
                } else if (key !== "_id" && obj[key]) {
                  totalhtml += "<strong><u>" + String(key) + "</strong></u>: " + String(value) + "<br>"
                }
              }
        }

        totalhtml += "</div></div>"


    }
    document.getElementById('metadata_entries').innerHTML = totalhtml
  }


  </script>
